<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.MainActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical"
        android:weightSum="9"
        >


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@color/black"
            app:srcCompat="@drawable/newlyadded"
            android:layout_weight="3"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:weightSum="3"
            android:orientation="vertical"
            android:layout_weight="6">

            <!--Line 1-->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="horizontal"
                android:weightSum="2"
                android:background="@color/white"
                android:layout_weight="1"
                android:id="@+id/line1">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:weightSum="5"
                    android:background="#ffffff"
                    android:id="@+id/line1Video">




                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        app:srcCompat="@drawable/videoes"
                        android:background="@color/colorOwlBlue"
                        android:layout_weight="4"
                        android:padding="20dp"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:background="#2A45CB"
                        android:text="Videos"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="20dp"

                        />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:weightSum="5"
                    android:background="@color/colorOwlBlue"
                    android:id="@+id/line1Audio">


                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        app:srcCompat="@drawable/audio"
                        android:layout_weight="4"
                        android:background="@color/colorOwlYellow"
                        android:padding="20dp"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:text="Audio"
                        android:textColor="@color/white"
                        android:background="#B79C02"
                        android:layout_weight="2"
                        android:textSize="20dp"
                        android:shadowColor="@color/black"
                        android:textAlignment="center"

                        />


                </LinearLayout>


            </LinearLayout>


            <!--Line 2-->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="horizontal"
                android:layout_weight="1"
                android:weightSum="2"
                android:background="@color/white"
                android:id="@+id/line2">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:weightSum="5"
                    android:background="#ffffff"
                    android:id="@+id/line2Notepad">


                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        app:srcCompat="@drawable/notepad"
                        android:background="@color/red"
                        android:layout_weight="4"
                        android:padding="20dp"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:text="Notepad"
                        android:textColor="@color/white"
                        android:background="#B11D05"
                        android:textSize="20dp"
                        android:layout_weight="1"
                        android:shadowColor="@color/black"
                        android:textAlignment="center"

                        />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:weightSum="5"
                    android:background="@color/colorOwlBlue"
                    android:id="@+id/line2Paint">


                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        app:srcCompat="@drawable/paint"
                        android:layout_weight="4"
                        android:background="@color/green"
                        android:padding="20dp"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:text="Paint"
                        android:textColor="@color/white"
                        android:background="#117705"
                        android:layout_weight="1"
                        android:textSize="20dp"
                        android:shadowColor="@color/black"
                        android:textAlignment="center"

                        />


                </LinearLayout>


            </LinearLayout>


            <!--Line 3-->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="horizontal"
                android:layout_weight="1"
                android:weightSum="2"
                android:background="@color/white"
                android:id="@+id/line3">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:background="#ffffff"
                    android:weightSum="5"
                    android:id="@+id/line3Camera">


                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        app:srcCompat="@drawable/camera"
                        android:layout_weight="4"
                        android:background="#E7316B"
                        android:padding="20dp"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:text="Camera"
                        android:textColor="@color/white"
                        android:background="#9C0636"
                        android:textSize="20dp"
                        android:layout_weight="1"
                        android:shadowColor="@color/black"
                        android:textAlignment="center"

                        />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:weightSum="5"
                    android:layout_weight="1"
                    android:background="@color/colorOwlBlue"
                    android:id="@+id/line3Others">


                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="4"
                        app:srcCompat="@drawable/others"
                        android:background="#A934D2"
                        android:padding="20dp"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:text="Others"
                        android:textColor="@color/white"
                        android:background="#4A0761"
                        android:layout_weight="1"
                        android:textSize="20dp"
                        android:shadowColor="@color/black"
                        android:textAlignment="center"

                        />


                </LinearLayout>


            </LinearLayout>

        </LinearLayout>


    </LinearLayout>

    <com.getbase.floatingactionbutton.FloatingActionsMenu
        android:id="@+id/multiple_actions"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/fab_margin"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="16dp"
        app:fab_addButtonColorNormal="#FF4500"
        app:fab_addButtonColorPressed="@color/white_pressed"
        app:fab_addButtonPlusIconColor="@color/white">

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/action_video"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:tint="@color/colorWhite"
            app:fab_colorNormal="@color/colorPrimaryPink"
            app:fab_colorPressed="@color/white_pressed"
            app:fab_icon="@drawable/ic_videocam_black_24dp"
            app:fab_title="VIDEO    " />

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/action_audio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:fab_colorNormal="@color/colorPrimaryPink"
            app:fab_colorPressed="@color/white_pressed"
            app:fab_icon="@drawable/ic_keyboard_voice_black_24dp"
            app:fab_title="AUDIO    " />


        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/action_notepad"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:fab_colorNormal="@color/colorPrimaryPink"
            app:fab_colorPressed="@color/white_pressed"
            app:fab_icon="@drawable/ic_note_add_black_24dp"
            app:fab_title="NOTEPAD    " />

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/action_paint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:fab_colorNormal="@color/colorPrimaryPink"
            app:fab_colorPressed="@color/white_pressed"
            app:fab_icon="@drawable/ic_color_lens_black_24dp"
            app:fab_title="PAINT   " />

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/action_camera"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:fab_colorNormal="@color/colorPrimaryPink"
            app:fab_colorPressed="@color/white_pressed"
            app:fab_icon="@drawable/ic_photo_camera_black_24dp"
            app:fab_title="CAMERA" />

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/action_others"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:fab_colorNormal="@color/colorPrimaryPink"
            app:fab_colorPressed="@color/white_pressed"
            app:fab_icon="@android:drawable/ic_dialog_dialer"
            app:fab_title="OTHERS" />

    </com.getbase.floatingactionbutton.FloatingActionsMenu>







</androidx.coordinatorlayout.widget.CoordinatorLayout>

